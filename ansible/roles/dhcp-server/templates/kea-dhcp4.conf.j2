{
"Dhcp4": {
    "interfaces-config": {
         {{ dhcp4_interfaces_config }}
    },
    "control-socket": {
        "socket-type": "unix",
        "socket-name": "/tmp/kea-dhcp4-ctrl.sock"
    },
    "lease-database": {
        {{ dhcp4_lease_database }}
    },

    "expired-leases-processing": {
        "reclaim-timer-wait-time": 10,
        "flush-reclaimed-timer-wait-time": 25,
        "hold-reclaimed-time": 3600,
        "max-reclaim-leases": 100,
        "max-reclaim-time": 250,
        "unwarned-reclaim-cycles": 5
    },
    "renew-timer": 900,
    "rebind-timer": 1800,
    "valid-lifetime": 3600,
    "option-data": [
        {
            "name": "boot-file-name",
            "data": "EST5EDT4\\,M3.2.0/02:00\\,M11.1.0/02:00"
        },
        {
            "name": "default-ip-ttl",
            "data": "0xf0"
        },
        {{ dhcp4_option_data }}
    ],
    "subnet4": [
        {{ dhcp4_subnets }}
    ],
	"dhcp-ddns": {
		"enable-updates": true,
		"server-ip": "10.120.69.18",
		"server-port": 53001,
		"sender-ip": "10.120.69.18",
		"sender-port": 0,
		"max-queue-size": 1024,
		"ncr-protocol": "UDP",
		"ncr-format": "JSON",
		"qualifying-suffix": "local.orzehhowski.pl",
		"override-client-update": true
	},
    "loggers": [
        {{ dhcp4_loggers }}
  ]
}
}
