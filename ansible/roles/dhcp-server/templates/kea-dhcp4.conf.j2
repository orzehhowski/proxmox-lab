{
"Dhcp4": {
    "interfaces-config": {
         {{ dhcp4_renders.interfaces_config }}
    },
    "control-socket": {
        "socket-type": "unix",
        "socket-name": "/tmp/kea-dhcp4-ctrl.sock"
    },
    "lease-database": {
        {{ dhcp4_renders.lease_database }}
    },

    "expired-leases-processing": {
        "reclaim-timer-wait-time": 10,
        "flush-reclaimed-timer-wait-time": 25,
        "hold-reclaimed-time": 3600,
        "max-reclaim-leases": 100,
        "max-reclaim-time": 250,
        "unwarned-reclaim-cycles": 5
    },
    "renew-timer": 900,
    "rebind-timer": 1800,
    "valid-lifetime": 3600,
    "option-data": [
        {
            "name": "boot-file-name",
            "data": "EST5EDT4\\,M3.2.0/02:00\\,M11.1.0/02:00"
        },
        {
            "name": "default-ip-ttl",
            "data": "0xf0"
        },
        {{ dhcp4_renders.option_data }}
    ],
    "subnet4": [
        {{ dhcp4_renders.subnets }}
    ],
    {% if ddns.use %}
	"dhcp-ddns": {
        {{ dhcp4_renders.ddns }}
	},
    {% endif %}
    "loggers": [
        {{ dhcp4_renders.loggers }}
  ]
}
}
