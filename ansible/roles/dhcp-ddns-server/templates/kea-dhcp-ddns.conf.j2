{
"DhcpDdns":
{
	"ip-address": "{{ ddns.server_ip }}",
	"port": {{ ddns.port }},
	"dns-server-timeout": 100,
	"control-socket": {
		"socket-type": "unix",
		"socket-name": "/tmp/kea-dhcp-ddns-ctrl.sock"
	 },
	"tsig-keys": [
        {{ ddns_renders.tsig_keys | from_yaml | to_json }}
	],
	"forward-ddns" : {
		"ddns-domains": [
            {{ ddns_renders.forward_domains | from_yaml | to_json }}
		]
	},
	"reverse-ddns" : {
		"ddns-domains": [
            {{ ddns_renders.reverse_domains | from_yaml | to_json }}
		]
	}
}
}
