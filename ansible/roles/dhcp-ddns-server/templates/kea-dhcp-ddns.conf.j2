{
"DhcpDdns":
{
	"ip-address": "{{ ddns_ip }}",
	"port": {{ ddns_port }},
	"dns-server-timeout": 100,
	"control-socket": {
		"socket-type": "unix",
		"socket-name": "/tmp/kea-dhcp-ddns-ctrl.sock"
	 },
	"tsig-keys": [
        {{ ddns_tsig_keys }}
	],
	"forward-ddns" : {
		"ddns-domains": [
            {{ ddns_forward_domains }}
		]
	},
	"reverse-ddns" : {
		"ddns-domains": [
            {{ ddns_reverse_domains }}
		]
	}
}
}
